{
  "name": "voiceprint_fusion_web",
  "version": "1.1.1",
  "description": "",
  "scripts": {
    "stylelint": "stylelint \"./src/**/*.less\"",
    "eslint": "eslint \"./src/**/*.ts\"",
    "types-check": "tsc",
    "lint": "npm run eslint && npm run stylelint && npm run types-check",
    "start": "npm run lint && cross-env NODE_ENV=development SITE=local node build/start.js",
    "build": "npm run lint && cross-env NODE_ENV=production SITE=local node build/build.js",
    "prod": "npm run lint && cross-env NODE_ENV=production node build/build.js",
    "api": "spk api -o src/api/_api.ts && node build/fixedApi.js && tsc --build src/api/config.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.(ts|tsx)": "eslint",
    "src/**/*.less": "stylelint"
  },
  "author": {
    "name": "wooline",
    "email": "wooline@qq.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=9.0.0"
  },
  "browserslist": [
    "chrome >= 49"
  ],
  "baseConf": {
    "siteName": "SPA Demo",
    "clientPublicPath": "/",
    "server": "http://localhost:7445",
    "mock": true,
    "proxy": {
      "/api/**": {
        "target": "http://192.168.0.232:8000",
        "pathRewrite": {
          "^/api": ""
        },
        "xfwd": true,
        "secure": false,
        "changeOrigin": true
      }
    }
  },
  "dependencies": {
    "@medux/react-web-router": "~0.2.23",
    "@types/lodash": "~4.14.149",
    "antd": "~3.26.0",
    "cookie-parser": "~1.4.4",
    "lodash": "~4.17.15",
    "path-to-regexp": "~6.1.0",
    "react": "~16.9.0",
    "react-dom": "~16.9.0"
  },
  "devDependencies": {
    "@medux/dev-pkg": "~0.2.22",
    "@medux/dev-utils": "~0.2.22",
    "@types/react": "~16.9.0",
    "@types/react-dom": "~16.9.0",
    "babel-plugin-import": "~1.12.0",
    "less": "~3.10.1",
    "less-loader": "~5.0.0",
    "postcss-less": "~3.1.4",
    "sass-resources-loader": "~2.0.1",
    "typescript": "~3.7.3"
  }
}
