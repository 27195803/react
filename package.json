{
  "name": "voiceprint_fusion_web",
  "version": "1.1.1",
  "description": "",
  "scripts": {
    "stylelint": "stylelint \"./src/**/*.less\"",
    "eslint": "eslint \"./src/**/*.ts\"",
    "types-check": "tsc",
    "lint": "npm run eslint && npm run stylelint && npm run types-check",
    "start": "npm run lint && cross-env NODE_ENV=development SITE=local node build/start.js",
    "build": "npm run lint && cross-env NODE_ENV=production SITE=local node build/build.js",
    "prod": "npm run lint && cross-env NODE_ENV=production node build/build.js",
    "api": "spk api -o src/api/_api.ts && node build/fixedApi.js && tsc --build src/api/config.json"
  },
  "author": {
    "name": "speakin",
    "email": "owner@speakin.mobi"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=9.0.0"
  },
  "browserslist": [
    "chrome > 56"
  ],
  "baseConf": {
    "siteName": "SPA Demo",
    "clientPublicPath": "/",
    "server": "http://localhost:7445",
    "mock": false,
    "proxy": {
      "/api/**": {
        "target": "http://192.168.0.232:8000",
        "pathRewrite": {
          "^/api": ""
        },
        "xfwd": true,
        "secure": false,
        "changeOrigin": true
      }
    }
  },
  "dependencies": {
    "@ant-design/dark-theme": "^1.0.3",
    "@babel/runtime": "~7.4.5",
    "@medux/react-web-router": "~0.2.12",
    "antd": "~3.23.1",
    "babel-polyfill": "~6.26.0",
    "core-js": "~3.1.3",
    "react": "~16.8.6",
    "react-dom": "~16.8.6"
  },
  "devDependencies": {
    "@medux/dev-pkg": "~0.2.11",
    "@medux/dev-utils": "~0.2.11",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "babel-plugin-import": "~1.12.0",
    "less": "~3.10.1",
    "less-loader": "~5.0.0",
    "postcss-less": "~3.1.4",
    "sass-resources-loader": "~2.0.1",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "typescript": "~3.5.3"
  }
}
